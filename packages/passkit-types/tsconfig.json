{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2020",
    "declaration": true,
    "sourceMap": true,
    "outDir": "lib",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
  }
}
